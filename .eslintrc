{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "plugin:prettier/recommended",
    "eslint:recommended",
    "prettier/react",
    "plugin:@typescript-eslint/recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "jest": true
  },
  "rules": {
    "react/prop-types": 0
  },
  "globals": {
  },
  "ignorePatterns": [
    "*.test.ts",
    "*.d.ts",
    "babel.config.js",
    "jest.config.js",
    "commitlint.config.js",
    "build/*.js"
  ]
}
